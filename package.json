{
  "name": "fit-onlinetour",
  "version": "0.0.1",
  "description": "fit onlinetour hybrid app",
  "productName": "FIT OnlineTour",
  "cordovaId": "kr.co.onlinetour.fit",
  "author": "Junho Nam <nowonlyhere@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:and": "quasar dev -m cordova -T android",
    "dev:ios": "quasar dev -m cordova -T ios",
    "build:web:prod": "./node_modules/.bin/cross-env NODE_ENV=production quasar build",
    "build:web:test": "./node_modules/.bin/cross-env NODE_ENV=test quasar build",
    "build:and:prod": "./node_modules/.bin/cross-env NODE_ENV=production quasar build -m cordova -T android",
    "build:and:test": "./node_modules/.bin/cross-env NODE_ENV=test quasar build -m cordova -T android",
    "build:ios:prod": "./node_modules/.bin/cross-env NODE_ENV=production quasar build -m cordova -T ios",
    "build:ios:test": "./node_modules/.bin/cross-env NODE_ENV=test quasar build -m cordova -T ios",
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "partial-js": "^1.1.8",
    "quasar": "^1.0.0-beta.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-beta.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "strip-ansi": "=3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  }
}
